{
  "name": "hum-drum",
  "version": "0.0.3",
  "description": "A small framework for playing music using the Web Audio API",
  "main": "index.js",
  "scripts": {
    "test": "ava tests/**/*.js --verbose",
    "start": "concurrently \"rollup -c -w\" \"hs -o -c-1\"",
    "rollup": "rollup -c rollup.config.js && rollup -c rollup.config.web.js",
    "install": "npm run rollup"
  },
  "keywords": [
    "webaudio",
    "audio"
  ],
  "author": {
    "name": "Bill Reid"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bl-rd/hum-drum"
  },
  "license": "ISC",
  "dependencies": {
    "rollup": "^0.45.2",
    "rollup-plugin-node-resolve": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^0.21.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-latest": "^6.24.1",
    "chalk": "^2.1.0",
    "concurrently": "^3.5.0",
    "fs-extra": "^4.0.1",
    "http-server": "^0.10.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-watch": "^4.3.1",
    "uglify-js": "^3.0.27"
  }
}
